git remote add origin https://github.com/vijaibaskarc/devops.git
      #To check the added ORIGIN :
        #	$git remote -v
        #	origin  https://github.com/poorniaws02/Poorni.git (fetch)
        #	origin  https://github.com/poorniaws02/Poorni.git (push)
       
       # IF ORIGIN is NOT added properly, you will encounter below error:
        #	C:\>git push -u origin master
        #	fatal: 'origin' does not appear to be a git repository
        #	fatal: Could not read from remote repository.

        #	Please make sure you have the correct access rights
        #	and the repository exists.

git push -u origin master
